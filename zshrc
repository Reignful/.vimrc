alias ls='ls -G'
alias l='ls -G | sort'
alias h='history'
alias p='python3'
alias python="/usr/local/bin/python3"
alias gitview="gh repo view --web"

alias editp='vim ~/.zshrc'
alias setp='source ~/.zshrc'

alias bupdate="brew cleanup && brew update && brew upgrade"

alias simulator='open -a Simulator'
